<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" class="form-control" name="newPostTitle" id="postTitle" value="" autocomplete="off" autofocus>
  </div>
  <div class="form-group">
    <label for="postMsg">Post</label>
    <textarea class="form-control" type="text" name="newPostMsg" id="postMsg" rows="3" autocomplete="off" autofocus></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>
